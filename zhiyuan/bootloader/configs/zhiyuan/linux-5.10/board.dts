/dts-v1/;
#include <dt-bindings/gpio/sun4i-gpio.h>

#include "sun50iw9.dtsi"

/{
	model = "sun50iw9";
	compatible = "allwinner,t507", "arm,sun50iw9p1";

	aliases {
		serial0 = &uart0;
		disp = &disp;
		lcd0 = &lcd0;
		pmu0 = &pmu0;
		standby_param = &standby_param;
		hdmi = &hdmi;
		spi0 = &spi0;
		spi1 = &spi1;
		twi0 = &twi0;
		twi1 = &twi1;
		twi2 = &twi2;
		twi3 = &twi3;
		twi4 = &twi4;
		twi5 = &twi5;
	};


	reg_usb1_vbus: usb1-vbus {
		compatible = "regulator-fixed";
		gpio = <&pio PE 19 GPIO_ACTIVE_HIGH>;
		regulator-name = "usb1-vbus";
		regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;
		regulator-enable-ramp-delay = <1000>;
		enable-active-high;
	};

       led: led {
	  pinctrl-names = "default";
          pinctrl-0 = <&led_pe_pins>;
          status = "okay";
        };
};

&cpu0 {
	cpu-supply = <&reg_dcdc2>;
};

&uart0 {
	pinctrl-names = "default", "sleep";
	pinctrl-0 = <&uart0_ph_pins>;
	pinctrl-1 = <&uart0_ph_sleep>;
	status = "okay";
};

&uart2 {
        pinctrl-names = "default", "sleep";
        pinctrl-0 = <&uart2_ph_pins>;
        pinctrl-1 = <&uart2_ph_sleep>;
        status = "okay";
};


&uart5 {
        pinctrl-names = "default", "sleep";
        pinctrl-0 = <&uart5_ph_pins>;
        pinctrl-1 = <&uart5_ph_sleep>;
        status = "okay";
};



&pio {
	sdc0_pins_a: sdc0@0 {
		allwinner,pins = "PF0", "PF1", "PF2",
				 "PF3", "PF4", "PF5";
		allwinner,function = "sdc0";
		allwinner,muxsel = <2>;
		allwinner,drive = <3>;
		allwinner,pull = <1>;
	};

	sdc2_pins_a: sdc2@0 {
		allwinner,pins = "PC1", "PC5", "PC6",
				 "PC8", "PC9", "PC10", "PC11",
				 "PC13", "PC14", "PC15", "PC16";
		allwinner,function = "sdc2";
		allwinner,muxsel = <3>;
		allwinner,drive = <3>;
		allwinner,pull = <1>;
	};

	sdc2_pins_c: sdc2@2 {
				allwinner,pins = "PC0";
				allwinner,function = "sdc2";
				allwinner,muxsel = <3>;
				allwinner,drive = <3>;
				allwinner,pull = <2>;
	};

	spi0_pins_a: spi0@0 {
		pins = "PC0", "PC2", "PC4", "PC15", "PC16";     /*CLK  MOSI  MISO WP  HOLD*/
		function = "spi0";
		muxsel = <4>;
		drive-strength = <10>;
	};

	spi0_pins_b: spi0@1 {
		pins = "PC3", "PC7";    /*CS0  CS1*/
		function = "spi0";
		muxsel = <4>;
		drive-strength = <10>;
		bias-pull-up;   /* only CS should be pulled up */
	};

	spi0_pins_c: spi0@2 {
		pins = "PC0", "PC2", "PC3", "PC4", "PC7", "PC15", "PC16";
		function = "gpio_in";
		muxsel = <0>;
		drive-strength = <10>;
	};

	spi1_pins_a: spi1@0 {
		pins = "PH6", "PH7", "PH8";     /*CLK  MOSI  MISO*/
		function = "spi1";
		muxsel = <4>;
		drive-strength = <10>;
	};

	spi1_pins_b: spi1@1 {
		pins = "PH5", "PH9";    /*CS0  CS1*/
		function = "spi1";
		muxsel = <4>;
		drive-strength = <10>;
		bias-pull-up;   /* only CS should be pulled up */
	};

	spi1_pins_c: spi1@2 {
		pins = "PH5", "PH6", "PH7", "PH8", "PH9";       /*CS0  CS1*/
		function = "gpio_in";
		muxsel = <0>;
		drive-strength = <10>;
	};

	gmac0_pins_a: gmac0@0 {
		pins = "PI0", "PI1", "PI2", "PI3",
			"PI4", "PI5", "PI7", "PI8",
			"PI9", "PI10", "PI11", "PI12",
			"PI13", "PI14", "PI15",
			"PI16";
		function = "emac0";
		drive-strength = <40>;
		bias-pull-up;
	};

	gmac0_pins_b: gmac0@1 {
		pins = "PI0", "PI1", "PI2", "PI3",
			"PI4", "PI5", "PI7",
			"PI8", "PI9", "PI10", "PI11",
			"PI12", "PI13", "PI14", "PI15",
			"PI16";
		function = "gpio_in";
		drive-strength = <40>;
	};

	gmac1_pins_a: gmac1@0 {
		pins = "PA0", "PA1", "PA2", "PA3",
			"PA4", "PA5", "PA6", "PA7",
			"PA8", "PA9";
		function = "gmac1";
		drive-strength = <10>;
		bias-pull-up;
	};

	gmac1_pins_b: gmac1@1 {
		pins = "PA0", "PA1", "PA2", "PA3",
			"PA4", "PA5", "PA6", "PA7",
			"PA8", "PA9";
		function = "gpio_in";
		drive-strength = <10>;
	};

	twi0_pins_a: twi0@0 {
		pins = "PA0", "PA1";
		function = "twi0";
		muxsel = <4>;
		drive-strength = <10>;
	};

	twi0_pins_b: twi0@1 {
		pins = "PA0", "PA1";
		function = "gpio_in";
		muxsel = <0>;
	};

	twi1_pins_a: twi1@0 {
		pins = "PI7", "PI8";
		function = "twi1";
		muxsel = <5>;
		drive-strength = <10>;
		allwinner,pull = <1>;
	};

	twi1_pins_b: twi1@1 {
		pins = "PI7", "PI8";
		function = "gpio_in";
		muxsel = <0>;
	};

	twi2_pins_a: twi2@0 {
		pins = "PE20", "PE21";
		function = "twi2";
		muxsel = <5>;
		drive-strength = <10>;
		allwinner,pull = <1>;
	};

	twi2_pins_b: twi2@1 {
		pins = "PE20", "PE21";
		function = "gpio_in";
		muxsel = <0>;
	};

	twi3_pins_a: twi3@0 {
		pins = "PA10", "PA11";
		function = "twi3";
		muxsel = <2>;
		drive-strength = <10>;
		allwinner,pull = <1>;
	};

	twi3_pins_b: twi3@1 {
		pins = "PA10", "PA11";
		function = "gpio_in";
		muxsel = <0>;
	};

	twi4_pins_a: twi4@0 {
		pins = "PG15", "PG16";
		function = "twi4";
		muxsel = <5>;
		drive-strength = <10>;
	};

	twi4_pins_b: twi4@0 {
		pins = "PG15", "PG16";
		function = "gpio_in";
		muxsel = <0>;
	};


        led_pe_pins: led_pe_pins {
                pins = "PE3", "PE0","PE20";
                function = "gpio_out";
                muxsel = <0>;
        };

        

	pwm0_pin_a: pwm0@0 {
		pins = "PD28";
		function = "pwm0";
		muxsel = <2>;
		drive-strength = <10>;
		bias-pull-up;
	};

	pwm0_pin_b: pwm0@1 {
		pins = "PD28";
		muxsel = <7>;
		function = "gpio_in";
	};

	clk_losc_pins_a: clk_losc@0 {
		allwinner,pins = "PG10";
		allwinner,function = "x32kfout";
		allwinner,muxsel = <3>;
		allwinner,drive = <2>;
		allwinner,pull = <1>;
	};

	uart1_pg_pins: uart1-ph-pins {
		pins = "PG6", "PG7", "PG8", "PG9";
		function = "uart1";
	};

	uart1_pg_sleep: uart1-ph-sleep {
		pins = "PG6", "PG7", "PG8", "PG9";
		function = "gpio_in";
	};


        uart2_ph_pins: uart2-ph-pins {
                pins = "PG15", "PG16";
                function = "uart2";
               };

        uart2_ph_sleep: uart2-ph-sleep {
                pins = "PG15", "PG16";
                function = "gpio_in";
             };

        uart5_ph_pins: uart5-ph-pins {
                pins = "PH2", "PH3";
                function = "uart5";
               };

        uart5_ph_sleep: uart5-ph-sleep {
                pins = "PH2", "PH3";
                function = "gpio_in";
             };



        spdif_pins_a: spdif@0 {
                pins = "PH4";   /* spdif out */
		function = "spdif";
		drive-strength = <20>;
		bias-disable;
        };

        spdif_pins_b: spdif_sleep@0 {
		pins = "PH4";
		function = "gpio_in";
                drive-strength = <20>;
                bias-disable;
	};

	ahub_daudio0_pins_a: ahub_daudio0@0 {
		pins = "PA6", "PA7", "PA8", "PA9";
		function = "i2s0";
		drive-strength = <20>;
		bias-disable;
	};

	ahub_daudio0_pins_b: ahub_daudio0_sleep@0 {
		pins = "PA6", "PA7", "PA8", "PA9";
		function = "gpio_in";
		drive-strength = <20>;
		bias-disable;
	};

	ahub_daudio2_pins_a: ahub_daudio2@0 {
		pins = "PG11", "PG12";
		function = "i2s2";
		drive-strength = <20>;
		bias-disable;
	};

	ahub_daudio2_pins_b: ahub_daudio2@1 {
		pins = "PG13";
		function = "i2s2_dout0";
		drive-strength = <20>;
		bias-disable;
	};

	ahub_daudio2_pins_c: ahub_daudio2@2 {
		pins = "PG14";
		function = "i2s2_din0";
		drive-strength = <20>;
		bias-disable;
	};

	ahub_daudio2_pins_d: ahub_daudio2_sleep@0 {
		pins = "PG11", "PG12", "PG13", "PG14";
		function = "gpio_in";
		drive-strength = <20>;
		bias-disable;
	};

	ahub_daudio3_pins_a: ahub_daudio3@0 {
		pins = "PH5", "PH6", "PH7";
		function = "i2s3";
		drive-strength = <20>;
		bias-disable;
	};

	ahub_daudio3_pins_b: ahub_daudio3@1 {
		pins = "PH8";
		function = "i2s3_dout0";
		drive-strength = <20>;
		bias-disable;
	};

	ahub_daudio3_pins_c: ahub_daudio3@2 {
		pins = "PH9";
		function = "i2s3_din0";
		drive-strength = <20>;
		bias-disable;
	};

	ahub_daudio3_pins_d: ahub_daudio3_sleep@0 {
		pins = "PH5", "PH6", "PH7", "PH8", "PH9";
		function = "gpio_in";
		drive-strength = <20>;
		bias-disable;
	};
};

&r_pio {
	s_twi0_pins_a: s_twi0@0 {
		pins = "PL0", "PL1";
		function = "s_twi0";
		drive-strength = <10>;
		muxsel = <3>;
		bias-pull-up;
	};

	s_twi0_pins_b: s_twi0@1 {
		pins = "PL0", "PL1";
		function = "gpio_in";
		muxsel = <0>;
	};
};

&cpu_trips{
	cpu_crit: cpu_crit@0 {
		temperature = <120000>;
		type = "critical";
		hysteresis = <0>;
	};
};

&gmac0 {
	phy-mode = "rgmii";
	pinctrl-names = "default", "sleep";
	pinctrl-0 = <&gmac0_pins_a>;
	pinctrl-1 = <&gmac0_pins_b>;
	sunxi,phy-clk-type = <0>;
	tx-delay = <7>;
	rx-delay = <0>;
	gmac-power0-supply = <&reg_cldo3>;
	gmac-power1-supply;
	gmac-power2-supply;
	/*gmac-power1-supply = <&reg_dcdc6>;*/
	gmac-power0-vol = <3300000>;
	gmac-power1-vol;
	gmac-power2-vol;
	status = "okay";
};

&soc {
	card0_boot_para@2 {
		/* Avoid dtc compiling warnings. */
		reg = <0x0 0x2 0x0 0x0>;
		device_type = "card0_boot_para";
		card_ctrl = <0x0>;
		card_high_speed = <0x1>;
		card_line = <0x4>;
		pinctrl-0 = <&sdc0_pins_a>;
	};

	card2_boot_para@3 {
		/* Avoid dtc compiling warnings. */
		reg = <0x0 0x3 0x0 0x0>;
		device_type = "card2_boot_para";
		card_ctrl = <0x2>;
		card_high_speed = <0x1>;
		card_line = <0x8>;
		pinctrl-0 = <&sdc2_pins_a &sdc2_pins_c>;
		sdc_ex_dly_used = <0x2>;
		sdc_io_1v8 = <0x0>;
		sdc_type = "tm4";
	};

	clk_pll_video2: pll_video2 {
		#clock-cells = <0>;
		compatible = "allwinner,pll-clock";
		lock-mode = "new";
		assigned-clocks = <&clk_pll_video2>;
		/*assigned-clock-rates = <432000000>;*/
		clock-output-names = "pll_video2";
	};

	clk_pll_de: pll_de {
		#clock-cells = <0>;
		compatible = "allwinner,pll-clock";
		assigned-clocks = <&clk_pll_de>;
		assigned-clock-rates = <696000000>;
		lock-mode = "new";
		clock-output-names = "pll_de";
	};

	clk_de: de {
		#clock-cells = <0>;
		compatible = "allwinner,periph-clock";
		assigned-clock-parents = <&clk_pll_de>;
		assigned-clock-rates = <696000000>;
		assigned-clocks = <&clk_de>;
		clock-output-names = "de";
	};

	clk_display_top: display_top {
		#clock-cells = <0>;
		compatible = "allwinner,periph-clock";
		clock-output-names = "display_top";
	};

	clk_tcon_lcd: tcon_lcd {
		#clock-cells = <0>;
		compatible = "allwinner,periph-clock";
		clock-output-names = "tcon_lcd";
	};

	clk_tcon_lcd1: tcon_lcd1 {
		#clock-cells = <0>;
		compatible = "allwinner,periph-clock";
		clock-output-names = "tcon_lcd1";
	};
	clk_tcon_tv: tcon_tv {
		#clock-cells = <0>;
		compatible = "allwinner,periph-clock";
		assigned-clock-parents = <&clk_pll_video2>;
		assigned-clocks = <&clk_tcon_tv>;
		clock-output-names = "tcon_tv";
	};
	clk_tcon_tv1: tcon_tv1 {
		#clock-cells = <0>;
		compatible = "allwinner,periph-clock";
		clock-output-names = "tcon_tv1";
	};
	clk_lvds: lvds {
		#clock-cells = <0>;
		compatible = "allwinner,periph-clock";
		clock-output-names = "lvds";
		assigned-clocks = <&clk_lvds>;
	};
	clk_hdmi: hdmi {
		#clock-cells = <0>;
		compatible = "allwinner,periph-clock";
		assigned-clock-parents = <&clk_pll_video2>;
		assigned-clocks = <&clk_hdmi>;
		clock-output-names = "hdmi";
	};
	clk_hdmi_slow: hdmi_slow {
		#clock-cells = <0>;
		compatible = "allwinner,periph-clock";
		assigned-clocks = <&clk_hdmi_slow>;
		clock-output-names = "hdmi_slow";
	};
	clk_hdmi_cec: hdmi_cec {
		#clock-cells = <0>;
		compatible = "allwinner,periph-clock";
		assigned-clocks = <&clk_hdmi_cec>;
		clock-output-names = "hdmi_cec";
	};
	clk_hdmi_hdcp: hdmi_hdcp {
		#clock-cells = <0>;
		compatible = "allwinner,periph-clock";
		assigned-clock-parents = <&clk_pll_periph1>;
		assigned-clocks = <&clk_hdmi_hdcp>;
		clock-output-names = "hdmi_hdcp";
	};
	clk_pll_periph1: pll_periph1 {
		#clock-cells = <0>;
		compatible = "allwinner,pll-clock";
		assigned-clocks = <&clk_pll_periph1>;
		assigned-clock-rates = <600000000>;
		lock-mode = "new";
		clock-output-names = "pll_periph1";
	};

	auto_print@54321 {
		reg = <0x0 0x54321 0x0 0x0>;
		device_type = "auto_print";
		status = "disabled";
	};

	vind0:vind@6600800 {
		csi_top = <324000000>;
		status = "okay";

		csi_cci0:cci@6614000 {
			status = "disabled";
		};
		csi_cci1:cci@6614400 {
			status = "disabled";
		};
		actuator0:actuator@6614890 {
			device_type = "actuator0";
			actuator0_name = "ad5820_act";
			actuator0_slave = <0x18>;
			actuator0_af_pwdn = <>;
			actuator0_afvdd = "afvcc-csi";
			actuator0_afvdd_vol = <2800000>;
			status = "disabled";
		};
		flash0:flash@66148a0 {
			device_type = "flash0";
			flash0_type = <2>;
			flash0_en = <>;
			flash0_mode = <>;
			flash0_flvdd = "";
			flash0_flvdd_vol = <>;
			device_id = <0>;
			status = "disabled";
		};
		vinc0:vinc@6609000 {
			vinc0_csi_sel = <0>;
			vinc0_mipi_sel = <0>;
			vinc0_isp_sel = <0>;
			vinc0_isp_tx_ch = <0>;
			vinc0_rear_sensor_sel = <0>;
			vinc0_front_sensor_sel = <0>;
			vinc0_sensor_list = <0>;
			status = "okay";
		};
		vinc1:vinc@6609200 {
			vinc1_csi_sel = <0>;
			vinc1_mipi_sel = <0>;
			vinc1_isp_sel = <0>;
			vinc1_isp_tx_ch = <1>;
			vinc1_rear_sensor_sel = <0>;
			vinc1_front_sensor_sel = <0>;
			vinc1_sensor_list = <0>;
			status = "okay";
		};
		vinc2:vinc@6609400 {
			vinc2_csi_sel = <1>;
			vinc2_mipi_sel = <0xff>;
			vinc2_isp_sel = <1>;
			vinc2_isp_tx_ch = <0>;
			vinc2_rear_sensor_sel = <1>;
			vinc2_front_sensor_sel = <1>;
			vinc2_sensor_list = <0>;
			status = "okay";
		};
		vinc3:vinc@6609600 {
			vinc3_csi_sel = <1>;
			vinc3_mipi_sel = <0xff>;
			vinc3_isp_sel = <1>;
			vinc3_isp_tx_ch = <1>;
			vinc3_rear_sensor_sel = <1>;
			vinc3_front_sensor_sel = <1>;
			vinc3_sensor_list = <0>;
			status = "okay";
		};
		vinc4:vinc@6609800 {
			vinc4_csi_sel = <1>;
			vinc4_mipi_sel = <0xff>;
			vinc4_isp_sel = <1>;
			vinc4_isp_tx_ch = <2>;
			vinc4_rear_sensor_sel = <1>;
			vinc4_front_sensor_sel = <1>;
			vinc4_sensor_list = <0>;
			status = "okay";
		};
		vinc5:vinc@6609A00 {
			vinc5_csi_sel = <1>;
			vinc5_mipi_sel = <0xff>;
			vinc5_isp_sel = <1>;
			vinc5_isp_tx_ch = <3>;
			vinc5_rear_sensor_sel = <1>;
			vinc5_front_sensor_sel = <1>;
			vinc5_sensor_list = <0>;
			status = "okay";
		};
	};

	/* audio dirver module -> external codec */
	td100_audio:td100_audio {
		#sound-dai-cells = <0>;
		compatible		= "Allwinnertech,td100_audio";
		status			= "disabled";
	};

	car_reverse:car-reverse {
		compatible  = "allwinner,sunxi-car-reverse";
		tvd_id  = <0x2>;
		screen_width = <1280>;
		screen_height = <800>;
		rotation = <0x0>;
		source = <0x1>;
		oview = <0x0>;
		oview_type = <0x4>;
		td104_used = <0x0>;
		reverse_pin = <&pio PI 15 6 0 1 0>;
		status      = "disabled";
	};
};

&keyboard {
	key_cnt = <5>;
	key0 = <212 115>;
	key1 = <409 114>;
	key2 = <592 139>;
	key3 = <745 28>;
	key4 = <882 172>;
	wakeup-source;
	status = "okay";
};

&pwm0 {
	pinctrl-names = "active", "sleep";
	pinctrl-0 = <&pwm0_pin_a>;
	pinctrl-1 = <&pwm0_pin_b>;
};

&gpadc {
	channel_num = <4>;
	channel_select = <1>;
	channel_data_select = <0>;
	channel_compare_select = <1>;
	channel_cld_select = <1>;
	channel_chd_select = <0>;
	channel0_compare_lowdata = <1700000>;
	channel0_compare_higdata = <1200000>;
	channel1_compare_lowdata = <460000>;
	channel1_compare_higdata = <1200000>;
	status = "disabled";
};

&spi0 {
	clock-frequency = <100000000>;
	pinctrl-names = "default", "sleep";
	pinctrl-0 = <&spi0_pins_a &spi0_pins_b>;
	pinctrl-1 = <&spi0_pins_c>;
	spi_slave_mode = <0>;
	status = "disabled";
};

&spi1 {
	clock-frequency = <100000000>;
	pinctrl-names = "default", "sleep";
	pinctrl-0 = <&spi1_pins_a &spi1_pins_b>;
	pinctrl-1 = <&spi1_pins_c>;
	spi_slave_mode = <0>;
