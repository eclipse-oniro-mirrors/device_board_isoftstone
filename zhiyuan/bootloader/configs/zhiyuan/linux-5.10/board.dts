&sdc1 {
	bus-width = <4>;
	no-mmc;
	no-sd;
	cap-sd-highspeed;
	/*sd-uhs-sdr12*/
	sd-uhs-sdr25;
	sd-uhs-sdr50;
	sd-uhs-ddr50;
	sd-uhs-sdr104;
	/*sunxi-power-save-mode;*/
	/*sunxi-dis-signal-vol-sw;*/
	cap-sdio-irq;
	keep-power-in-suspend;
	ignore-pm-notify;
	max-frequency = <150000000>;
	/*vmmc-supply = <&reg_dldo1>;*/
	status = "okay";
};

&uart1 {
	pinctrl-names = "default", "sleep";
	pinctrl-0 = <&uart1_pg_pins>;
	pinctrl-1 = <&uart1_pg_sleep>;
	device_type = "uart1";
	status = "okay";
};

&rfkill {
	compatible = "allwinner,sunxi-rfkill";
	chip_en;
	power_en;
	pinctrl-0     = <&clk_losc_pins_a>;
	pinctrl-names = "default";
	status = "okay";

	/* wlan session */
	wlan {
		compatible    = "allwinner,sunxi-wlan";
		clocks        = <&rtc_ccu CLK_OSC32K_OUT>;
		clock-names   = "osc32k-out";
		wlan_power    = "axp858-dcdc6";
		wlan_io_regulator = "axp858-cldo3";
		wlan_power_vol= <3300000>;
		wlan_busnum   = <0x1>;
		wlan_regon    = <&pio PE 17 GPIO_ACTIVE_HIGH>;
		wlan_hostwake = <&pio PE 16 GPIO_ACTIVE_HIGH>;
		wakeup-source;
	};

	/* bt session */
	bt {
		compatible    = "allwinner,sunxi-bt";
		clocks        = <&rtc_ccu CLK_OSC32K_OUT>;
		clock-names   = "osc32k-out";
		bt_power      = "axp858-dcdc1";
		bt_io_regulator = "axp858-cldo2";
		/*bt_power_vol  = <3300000>; */
		bt_rst_n      = <&pio PE 8 GPIO_ACTIVE_LOW>;
	};
};

&addr_mgt {
	compatible     = "allwinner,sunxi-addr_mgt";
	type_addr_wifi = <0x0>;
	type_addr_bt   = <0x0>;
	type_addr_eth  = <0x0>;
	status         = "okay";
};

&btlpm {
	compatible  = "allwinner,sunxi-btlpm";
	uart_index  = <0x1>;
	bt_wake     = <&pio PE 7 GPIO_ACTIVE_HIGH>;
	bt_hostwake = <&pio PE 6 GPIO_ACTIVE_HIGH>;
	wakeup-source;
	status      = "okay";
};

/* audio driver adapter -> harmony ADM */
